# UseKeychain is not recognized by openssh, but recognized by system ssh on Mac
IgnoreUnknown UseKeychain

Host github.com
	HostName github.com
	User git
	IdentityFile ~/.ssh/github_rsa
	UseKeychain yes


Match host *.corp.google.com
ProxyCommand /usr/local/bin/corp-ssh-helper %h %p

Match host *.c.googlers.com
ProxyCommand /usr/local/bin/corp-ssh-helper %h %p

Host *.corp.google.com *.corp *.googlers.com
	ForwardAgent yes
